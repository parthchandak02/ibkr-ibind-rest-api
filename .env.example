# IBKR REST API - Sample Environment Variables
# Copy this file to .env and modify as needed

# Specify the IBKR account ID (Optional, will be detected automatically if not set)
# IBIND_ACCOUNT_ID=your_account_id

# Authentication settings
IBIND_USE_OAUTH=true

# Trading environment: paper_trading or live_trading
IBIND_TRADING_ENV=paper_trading

# Optional: Customize the API port (default is 5001)
# PORT=5001

# Optional: Set log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
# LOG_LEVEL=INFO
